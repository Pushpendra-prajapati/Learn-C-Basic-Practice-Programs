// Linear search in array .

#include <iostream>
using namespace std;

int main(){
    int arr[10]={10,15,20,25,30,40,50,55,80,35};

    int check;
    cout<<"Enter Number :";
    cin>>check;    

    for(int i=0; i<10; i++){
        if(arr[i]==check){
            cout<<check<<" present at "<<i<<" index.";
        }
    }
    return 0;
}
